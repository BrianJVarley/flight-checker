{
  "extends": "../../tsconfig.base.json",
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ],
   "paths": {
      "@flight-workspace/dashboard-lib": ["libs/dashboard-lib/src/index.ts"],
      "@flight-workspace/flight-lib": ["libs/flight-lib/src/index.ts"],
      "@flight-workspace/logger-lib": ["libs/logger-lib/src/index.ts"],
      "@flight-workspace/passenger/domain": [
        "libs/passenger/domain/src/index.ts"
      ],
      "@flight-workspace/passenger/feature-edit": [
        "libs/passenger/feature-edit/src/index.ts"
      ],
      "@flight-workspace/passenger/feature-search": [
        "libs/passenger/feature-search/src/index.ts"
      ]
    },
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "angularCompilerOptions": {
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
